includes:
    - phpstan-baseline.neon

parameters:
    level: 8
    paths:
        - src
        - config
        - database
    tmpDir: build/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true

    ignoreErrors:
        - identifier: missingType.iterableValue
        - identifier: larastan.noEnvCallsOutsideOfConfig
          path: config/*

